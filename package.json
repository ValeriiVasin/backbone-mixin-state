    "babel-core": "^4.7.4",
    "babel-loader": "^5.0.0",
    "babelify": "^5.0.4",
    "backbone": "^1.1.2",
    "browserify": "^9.0.3",
    "jasmine-core": "^2.2.0",
    "jquery": "^2.1.3",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
